    "dreamfactory",
    "dsp",
    "rest",
    "iot",
    "mqtt",
    "mosquitto",
    "rabbitmq"
  ],
  "homepage":    "https://github.com/dreamfactorysoftware/df-mqtt",
  "license":     "proprietary",
  "authors":     [
    {
      "name":  "Arif Islam",
      "email": "arifislam@dreamfactory.com"
    }
  ],
  "support":     {
    "email":  "arifislam@dreamfactory.com",
    "source": "https://github.com/dreamfactorysoftware/df-mqtt",
    "issues": "https://github.com/dreamfactorysoftware/df-mqtt/issues",
    "wiki":   "https://github.com/dreamfactorysoftware/df-mqtt/wiki"
  },
  "require":     {
    "dreamfactory/df-pubsub": "dev-develop as dev-master"
